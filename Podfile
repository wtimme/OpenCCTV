# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'
inhibit_all_warnings!

target 'OpenStreetMapCompletionist' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OpenStreetMapCompletionist
  pod 'SwiftOverpass', :git => 'git@github.com:wtimme/SwiftOverpassWrapper.git', :branch => 'refactoring/Improve-OpenStreetMap-elements'
  pod 'SwiftIcons', '~> 2.0.1'
  pod 'SwiftLocation', '~> 3.2.1'
  pod 'Eureka', '~> 4.1.1'
  pod 'SDWebImage', '~> 4.0'
  pod 'Presentr', '~> 1.3.2'
  
  # Networking/API
  pod 'Alamofire', '~> 4.7.0'
  pod 'AEXML', '~> 4.2.2'
  
  # Loading indicator
  pod 'FTLinearActivityIndicator'

  pod 'OAuthSwift', '~> 1.2.0'
  pod 'KeychainAccess', '~> 3.1.0'
  pod 'SQLite.swift', '~> 0.11.4'
  
  pod 'SwiftFormat/CLI', '~> 0.33.5'

  pod 'OSMSwift', :path => './OSMSwift'

end
